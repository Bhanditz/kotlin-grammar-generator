  ;

modifier
  : classModifier
  : accessModifier
  : varianceAnnotation
  : memberModifier
  : parameterKind
  : attributes
  // sealed by default
  ;

classModifier
  : "abstract"
  : "virtual"
  : "enum"
  : "extendable" // TODO: open?
  : "attribute"
  ;

memberModifier
  : "override"
