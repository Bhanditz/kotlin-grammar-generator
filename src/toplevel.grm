[start] jetlFile
  : preamble toplevelObject*
  ;

[start] script
  : preamble expression*
  ;

preamble
  : namespaceHeader? import*
  ;

namespaceHeader
  : "namespace" SimpleName{"."}
  ;

import
  : "import" SimpleName{"."} ("." "*" | "as" SimpleName)?
  ;

toplevelObject
  : namespace
  : class
  : extension
  : function
  : property
  ;

namespace
  : "namespace" SimpleName{"."} "{"
       import*
       toplevelObject*
    "}"
  ;